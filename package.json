{
  "name": "@edcarroll/class-discriminators",
  "version": "1.0.0",
  "description": "Aids Typescript type checking for class based discriminated unions",
  "main": "dist/cjs/index.js",
  "types": "dist/es6/index.d.ts",
  "module": "dist/es6/index.js",
  "dependencies": {},
  "devDependencies": {
    "@edcarroll/code-style": "^2.0.0",
    "typescript": "^2.2.2"
  },
  "scripts": {
    "lint": "tslint -c tslint.json --type-check --project ./tsconfig.json",
    "compile": "tsc && tsc -p tsconfig.cjs.json",
    "compile:w": "tsc -w",
    "prepublishOnly": "npm run build",
    "build": "npm run lint && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edcarroll/class-discriminators.git"
  },
  "author": "Edward Carroll",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edcarroll/class-discriminators/issues"
  },
  "homepage": "https://github.com/edcarroll/class-discriminators#readme"
}
